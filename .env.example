# Environment Variables Template
# Copy this file to .env and fill in your actual values

# Database Configuration
DATABASE_URL=your_postgresql_database_url_here

# Session Configuration
SESSION_SECRET=your-super-secret-session-key-change-this-in-production

# Application Configuration
NODE_ENV=production
PORT=5000

# Example for Neon Database (recommended for Vercel):
# DATABASE_URL=postgresql://username:password@hostname/database?sslmode=require

# Example for Railway Database:
# DATABASE_URL=postgresql://postgres:password@hostname:port/database

# Example for Supabase Database:
# DATABASE_URL=postgresql://postgres:password@hostname:port/postgres